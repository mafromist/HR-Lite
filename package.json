{
  "name": "@lit/lit-starter-js",
  "private": true,
  "version": "2.0.2",
  "description": "A simple web component for HR Department",
  "main": "app.js",
  "module": "app.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint '**/*.js'",
    "lint:lit-analyzer": "lit-analyzer app.js",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"**/*.js\" --exclude docs",
    "analyze:watch": "cem analyze --litelement --globs \"**/*.js\" --exclude docs --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat app.bundled.js | gzip -9 | wc -c ; rm app.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript",
    "lit"
  ],
  "authors": [
    "Google LLC",
    "Muge Alev"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@vaadin/router": "^2.0.0",
    "lit": "^3.2.0",
    "lit-element-state": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@open-wc/building-rollup": "^1.9.4",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/rollup-plugin-copy": "^0.5.1",
    "@web/rollup-plugin-html": "^2.3.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.15.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^3.0.0"
  },
  "customElements": "custom-elements.json"
}
